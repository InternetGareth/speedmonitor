{
  "dashboard": {
    "id": null,
    "title": "Internet Speed Monitor",
    "tags": ["speedtest", "internet", "monitoring"],
    "timezone": "browser",
    "refresh": "5m",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Current Speed",
        "type": "stat",
        "targets": [
          {
            "refId": "A",
            "query": "from(bucket: \"speedtest\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"internet_speed\")\n  |> filter(fn: (r) => r[\"_field\"] == \"download_speed\")\n  |> last()",
            "datasource": {
              "type": "influxdb",
              "uid": "${DS_INFLUXDB}"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "Mbps",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 25
                },
                {
                  "color": "green",
                  "value": 50
                }
              ]
            }
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "horizontal"
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Current Upload Speed",
        "type": "stat",
        "targets": [
          {
            "refId": "A",
            "query": "from(bucket: \"speedtest\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"internet_speed\")\n  |> filter(fn: (r) => r[\"_field\"] == \"upload_speed\")\n  |> last()",
            "datasource": {
              "type": "influxdb",
              "uid": "${DS_INFLUXDB}"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "Mbps",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 10
                },
                {
                  "color": "green",
                  "value": 25
                }
              ]
            }
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "horizontal"
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Download Speed Trend",
        "type": "timeseries",
        "targets": [
          {
            "refId": "A",
            "query": "from(bucket: \"speedtest\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"internet_speed\")\n  |> filter(fn: (r) => r[\"_field\"] == \"download_speed\")\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)",
            "datasource": {
              "type": "influxdb",
              "uid": "${DS_INFLUXDB}"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "Mbps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
